<script>

$(document).ready(function(){

/*
$('.jcarousel').jcarousel({
        // Configuration goes here
    });
*/
    
    var scrollskip = 0;
    var scrollbool = true;
    (function($, Modernizr) {

        $(function() {

            $('.jcarousel').jcarousel({

                wrap: 'circular',

                transitions: Modernizr.csstransitions ? {

                    transforms:   Modernizr.csstransforms,

                    transforms3d: Modernizr.csstransforms3d,

                    easing:       'ease'

                } : false

            });

            
            function scrolltimer(){ 
                if ( scrollbool ){
                    $('.jcarousel').jcarousel('scroll', '+=1');
                }
                else{
                    
                    if ( (scrollskip + 1) % 4 == 0 ){
                        scrollskip = 0;
                        scrollbool = true;
                    }
                    else {
                        scrollskip += 1;
                    }
                }
            }
            
            setInterval(scrolltimer, 6000);
            
            
            /*
            $('.jcarousel-control-prev').jcarouselControl({

                target: '-=1'

            });



            $('.jcarousel-control-next').jcarouselControl({

                target: '+=1'

            });
            */
        });

    })(jQuery, Modernizr);
        
    $("#encvidid").on("ended", function(){
        $("#encvidid").get(0).currentTime = 5;
    });
     
    
    
    
    $("#carouselnav0").on("click", function()
    {
        $('.jcarousel').jcarousel('scroll', 0);
        scrollbool = false; scrollskip = 0;
    });

    $("#carouselnav1").on("click", function()
    {
        $('.jcarousel').jcarousel('scroll', 1);
        scrollbool = false; scrollskip = 0;
    });

    $("#carouselnav2").on("click", function()
    {
        $('.jcarousel').jcarousel('scroll', 2);
        scrollbool = false; scrollskip = 0;
    });
    
    // outerHeight
    // offset().top
    //window.height
    //window.scrollTop
    
    var $productimage = $('#product-image-id');
    var $storyimage = $('#story-image-id');
    var $window = $(window);
    var $encvid = $("#encvidid");
    var $productimagein = false;
    var $storyimagein = false;
    var $mbarlogo = $(".menubarlogo");
    var $animationinprogress = false;
    
    //$( "#product-image-id" ).fadeTo(5,0);
    
    $(window).on("scroll",function(){
        
//         console.log( "between " + $(window).scrollTop() + " and " + ((parseInt($(window).scrollTop())) + (parseInt($(window).height()))) + " " );
//         
//         console.log( "between " + parseInt($productimage.offset().top) + " and " + (parseInt($productimage.offset().top) + parseInt($productimage.outerHeight())) );
//         
//         console.log( "fire if " + $productimagein && parseInt($productimage.offset().top) + "<" + ((parseInt($(window).scrollTop())) + (parseInt($(window).height()))));
        
        if ( ! $productimagein && parseInt($productimage.offset().top) < ((parseInt($(window).scrollTop())) + (parseInt($(window).height()))) ){
            
            $( "#product-image-id" ).effect( "slide", {direction:"right", distance: 1000+ $productimage.offset().right + $productimage.width() }, 2000, endslide );
            $( "#product-image-scr-id").css({"visibility":"visible"});

            $productimagein = true;            
        }
        
        if ( ! $storyimagein && parseInt($storyimage.offset().top) < ((parseInt($(window).scrollTop())) + (parseInt($(window).height()))) ){
            
            $( "#story-image-id" ).effect( "slide", {direction:"left", distance: 1000+ $storyimage.offset().right + $storyimage.width() }, 2000, endslide );
            $( "#story-image-scr-id").css({"visibility":"visible"});

            $storyimagein = true;
            
        }
        
        //console.log( (parseInt($encvid.offset().top) + parseInt($encvid.height()))  );
        //console.log( ((parseInt($(window).scrollTop())))  );
        
        if ( (parseInt($encvid.offset().top) + parseInt($encvid.height())) - 60 > (parseInt($(window).scrollTop()))  )
        {
            //console.log("hidden");
            //$mbarlogo.css("visibility", "hidden").fadeIn("slow");
            if ( !$animationinprogress && $mbarlogo.css("opacity") != 0.0 ){
                $animationinprogress = true;
                $mbarlogo.animate({ opacity: 0.0 }, 400, "swing", function(){$animationinprogress=false;});
            }
        }
        else{
            //console.log("visible");
            if ( !$animationinprogress && $mbarlogo.css("opacity") != 1.0 ){
                $animationinprogress = true;
                $mbarlogo.animate({ opacity: 1.0 }, 400, "swing", function(){$animationinprogress=false;});
            }
            //$mbarlogo.css("visibility", "visible").fadeIn("slow");
        }
        
    });
    
    function endslide(){}
    
    
    
    
    $("#encvidid").on("click", function(){
        $(window).scrollTop( $(".jcarousel-wrapper-wrapper-wrapper").offset().top - 130);
    });
    
    $(".jcarousel-wrapper-wrapper-wrapper").on("click", function(event){
        if ( parseFloat(event.pageY - this.offsetTop) < parseFloat($(".jcarousel-wrapper-wrapper-wrapper").height()) - (parseFloat($(".jcarousel-wrapper-wrapper-wrapper").height()) / 5 ) ){
            $(window).scrollTop( $("#scrollmaincontents").offset().top - 130);
            //alert("scroll");
        }
        //alert(event.offsetY);
        
        //alert(event.pageY - this.offsetTop);
        //alert( parseFloat($(".jcarousel-wrapper-wrapper-wrapper").height()) - (parseFloat($(".jcarousel-wrapper-wrapper-wrapper").height()) / 5 ) )
        //$(".jcarousel-wrapper-wrapper-wrapper")
        //$(window).scrollTop( $("#scrollmaincontents").offset().top - 130);
    });
    
    
        
});



</script>


